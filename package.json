{
  "name": "@yipjunkai/strapi-provider-email-resend",
  "type": "module",
  "version": "0.0.1",
  "license": "MIT",
  "author": "Yip Jun Kai (https://yipjunkai.com)",
  "description": "A strapi email provider plugin for sending emails with resend",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "prepare": "npm run build",
    "build": "tsc",
    "check": "tsc --noEmit",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "resend": "^6.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.38.0"
  },
  "packageManager": "pnpm@10.13.1",
  "engines": {
    "node": ">=22"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yipjunkai/strapi-provider-email-resend.git"
  },
  "bugs": {
    "url": "https://github.com/yipjunkai/strapi-provider-email-resend/issues"
  },
  "homepage": "https://github.com/yipjunkai/strapi-provider-email-resend",
  "keywords": [
    "strapi",
    "email",
    "provider",
    "resend"
  ]
}
